{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "baseUrl": "./",
        "keepClassNames": true,
        "paths": {
            "@core/*": [
                "./src/code/*"
            ]
        }
    },
    "minify": false
}