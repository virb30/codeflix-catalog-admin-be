    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/sequelize": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "mysql2": "^3.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
