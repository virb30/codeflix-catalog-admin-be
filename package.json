    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.0",
    "@swc/jest": "^0.2.38",
    "@types/chance": "^1.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
